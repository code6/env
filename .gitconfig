[user]
	name = code6 
	email = wzc1989@gmail.com
[color]
    ui = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[alias]
	st = "status -s "
	co = checkout
    ls = "ls-files"
    ci = commit  
    br = branch -v  
    #rt = reset --hard  
    sb = "show-branch --sha1-name"
    unstage = reset HEAD  
    uncommit = reset --soft HEAD^  
    l = log --pretty=oneline --abbrev-commit --graph --decorate
    amend = commit --amend  
    who = shortlog -n -s --no-merges  
    g = grep -n --color -E  
    cp = cherry-pick -x  
    nb = checkout -b 
[core]
	filemode = false
	editor = /Users/zhichunw/homebrew/bin/vim
[http]
	postBuffer = 524288000
[credential]
	helper = osxkeychain
